<?xml version="1.0"?>
<launch>

<!-- <arg name="formation_type" value="left-right-facing"/> -->
<arg name="formation_type" value="antipodal"/>
<node pkg="user_server" type="user_server_node" name="user_server_node" output="screen">
    <!-- <param name="file_location" value="$(find user_server)/launch/mockmap0.pcd" /> -->
    <!-- <param name="file_location" value="$(find user_server)/launch/d2_obstacle_map.pcd" /> -->
    <param name="file_location" value="" />
    <param name="tracker_timer_hz" value="5.0"/>
    <param name="cloud_timer_hz" value="12.0"/>
    <param name="single_or_multi" value="multi"/>
    <param name="formation" value="$(arg formation_type)"/>
    <param name="agents" value="10"/>
    <rosparam param="waypoints">
        [0.0, -30.0, 2.0, 
        0.0, 20.0, 2.0,
        0.0, -30.0, 2.0,
        0.0, 20.0, 2.0]</rosparam>
</node>

<include file="$(find user_server)/launch/generated_$(arg formation_type)_4.launch" />

<node name="rviz" pkg="rviz" type="rviz" args="-d $(find quad)/rviz/generated_display_10.rviz"/>

</launch>